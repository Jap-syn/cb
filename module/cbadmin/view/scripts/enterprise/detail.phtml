  <script type="text/javascript">
    function send(eid)
    {
      if (confirm('審査結果メールを送信します。'))
      {
        var url = '<?php echo $this->urlSendExam; ?>';
        url = '<?php echo $this->baseUrl; ?>' + url + '/eid/' + eid;

        var ajax = new Ajax.Request(
          url,
          {
            method: 'get',
            asynchronous: false
          });

        var res = eval('(' + ajax.transport.responseText + ')');
        if  (res['status'] == 1)
        {
          alert('審査結果メールを送信しました。\n\nパスワードリセットを実行して事業者様へパスワードを通知してください');
        }
        else
        {
          alert('審査結果メールの送信に失敗しました。 Err=' + res['status']);
        }
      }
    }

    function sendTest(eid)
    {
      if (confirm('送達確認用メールを送信します。'))
      {
        var url = '<?php echo $this->urlSendTest; ?>';
        url = '<?php echo $this->baseUrl; ?>' + url + '/eid/' + eid;

        var ajax = new Ajax.Request(
          url,
          {
            method: 'get',
            asynchronous: false
          });

        var res = eval('(' + ajax.transport.responseText + ')');
        if  (res['status'] == 1)
        {
          alert('送達確認用メールを送信しました。');
        }
        else
        {
          alert('送達確認用メールの送信に失敗しました。 Err=' + res['status']);
        }
      }
    }
